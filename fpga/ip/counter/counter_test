    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x5605cd957350 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x5605cd957460_0 .net "clk", 0 0, v0x5605cd9a1240_0;  1 drivers
v0x5605cd9a1010_0 .var "out", 7 0;
v0x5605cd9a10f0_0 .net "reset", 0 0, v0x5605cd9a1310_0;  1 drivers
E_0x5605cd98cb60 .event edge, v0x5605cd9a10f0_0;
E_0x5605cd98ca20 .event posedge, v0x5605cd957460_0;
    .scope S_0x5605cd98db10;
T_0 ;
    %wait E_0x5605cd98ca20;
    %load/vec4 v0x5605cd9a1010_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x5605cd9a1010_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5605cd98db10;
T_1 ;
    %wait E_0x5605cd98cb60;
    %load/vec4 v0x5605cd9a10f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %cassign/vec4 v0x5605cd9a1010_0;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x5605cd9a1010_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5605cd98d980;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5605cd9a1310_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5605cd9a1240_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5605cd98d980;
T_3 ;
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5605cd98d980 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5605cd98d980;
T_4 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5605cd9a1310_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5605cd9a1310_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5605cd9a1310_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5605cd9a1310_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5605cd98d980;
T_5 ;
    %delay 5, 0;
    %load/vec4 v0x5605cd9a1240_0;
    %nor/r;
    %store/vec4 v0x5605cd9a1240_0, 0, 1;
